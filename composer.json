{
    "name": "win-local-inc/remote-cron",
    "description": "A package for remote cron.",
    "keywords": [
        "remote-cron"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kiszka",
            "email": "piotrek@lifeinmobile.com",
            "role": "Programmer"
        }
    ],
    "autoload": {
        "psr-4": {
            "WinLocal\\RemoteCron\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WinLocal\\RemoteCron\\RemoteCronServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "require": {
        "php": "^7.1|^8.0",
        "laravel/framework": "^v7.0.0|^v8.0.0|^v9.0.0|^v10.0.0"
    }
}